    <path pathString="EmbeddedJs" checkConnect="false" sfe="false" seoe="false">
      <scriptName>Fundo piscando</scriptName>
      <scriptText><![CDATA[importPackage(Packages.org.csstudio.opibuilder.scriptUtil); 
var pv0 = PVUtil.getDouble(pvs[0]);
var pv1 = PVUtil.getDouble(pvs[1]);
var pv2 = PVUtil.getDouble(pvs[2]);
var pv3 = PVUtil.getDouble(pvs[3]);
var count = PVUtil.getDouble(pvs[4]);

if(pv1 >= 2 || pv2 >= 2 || pv3 >= 2){
	if(pv0 == 1){
		widget.setPropertyValue("background_color","Major");
	}if(pv0 == 0){
		widget.setPropertyValue("background_color","Header MainColor");
	}count = 1;
}
if(count == 1){
	if(pv0 == 1){
		widget.setPropertyValue("background_color","Major");
	}if(pv0 == 0){
		widget.setPropertyValue("background_color","Header MainColor");
}}]]></scriptText>
      <pv trig="true">sim://ramp(0,1,1,0.5)</pv>
      <pv trig="true">RAD:Thermo1:TotalDoseRate:Dose-T</pv>
      <pv trig="true">RAD:Berthold:TotalDoseRate:Dose-T</pv>
      <pv trig="true">RAD:ELSE:TotalDoseRate:Dose-T</pv>
      <pv trig="true">loc://count(0)</pv>
    </path>
  </scripts>
